{
  "name": "@cubicler/cubicclientkit",
  "version": "1.0.0",
  "description": "A TypeScript client SDK for the Cubicler ecosystem",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cubicler",
    "ai",
    "client",
    "sdk",
    "typescript",
    "agents",
    "orchestrator"
  ],
  "author": "Cubicler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cubicler/CubicClientKit.git"
  },
  "homepage": "https://github.com/Cubicler/CubicClientKit#readme",
  "bugs": {
    "url": "https://github.com/Cubicler/CubicClientKit/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.54.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
